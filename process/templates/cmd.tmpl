{{ $path := .Path }}
{{ $imports := .Imports }}
package main

import (
	"kego.io/process"

	_ "kego.io/system"
    _ "kego.io/system/types"

	{{ if not (eq $path "kego.io/system") }}
		_ "{{ $path }}"
	{{ end }}

	{{ range $importName, $importPath := $imports }}
		_ "{{ $importPath }}"
		_ "{{ $importPath }}/types"
	{{ end }}
)

func main() {
	if err := process.GenerateFiles(process.F_TYPES); err != nil {
		panic(err)
	}
}